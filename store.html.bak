panelHeading.innerHTML = 'BLACK FRIDAY DEAL';
panel.appendChild(panelHeading);
var panelBody = document.createElement('div');
panelBody.className = 'panel-body';
panel.appendChild(panelBody);
var img = document.createElement('img');
img.src = 'https://source.unsplash.com/random/5';
img.className = 'img-responsive';
img.style.width = '100%';
img.alt = 'Image';
panelBody.appendChild(img);
var panelFooter = document.createElement('div');
panelFooter.className = 'panel-footer';
panelFooter.innerHTML = 'Buy 50 mobiles and get a gift card';
panel.appendChild(panelFooter);
/* crear otros 11 */
var col2 = col.cloneNode(true);
row.appendChild(col2);
var col3 = col.cloneNode(true);
row.appendChild(col3);
var col4 = col.cloneNode(true);
row.appendChild(col4);
var col5 = col.cloneNode(true);
row.appendChild(col5);
var col6 = col.cloneNode(true);
row.appendChild(col6);
var col7 = col.cloneNode(true);
row.appendChild(col7);
var col8 = col.cloneNode(true);
row.appendChild(col8);
var col9 = col.cloneNode(true);
row.appendChild(col9);
var col10 = col.cloneNode(true);
row.appendChild(col10);
var col11 = col.cloneNode(true);
row.appendChild(col11);
var col12 = col.cloneNode(true);
row.appendChild(col12);
/* con image diferente */
var cols = document.getElementsByClassName('col-sm-4');
for (var i = 0; i < cols.length; i++) {
  var img = cols[i].getElementsByTagName('img')[0];
  img.src = 'https://source.unsplash.com/random/' + (i + 1) + '?w=800' + (i + 1);
  img.style.width = '100%';
  img.style.height = '500px';
  img.style.objectFit = 'cover';
  img.parentNode.parentNode.parentNode.style.cursor = 'pointer';
  img.parentNode.parentNode.parentNode.onclick = function() {
    window.open('https://www.facebook.com/', '_blank');
  };
  img.parentNode.parentNode.parentNode.onmouseover = function() {
    this.style.opacity = '0.5';
  };
  img.parentNode.parentNode.parentNode.onmouseout = function() {
    this.style.opacity = '1';
  };
  img.parentNode.parentNode.parentNode.style.transition = 'all 0.5s';
}
var flipBox = document.createElement('div');
flipBox.className = 'flip-box';
var flipBoxInner = document.createElement('div');
flipBoxInner.className = 'flip-box-inner';
flipBox.appendChild(flipBoxInner);
var flipBoxFront = document.createElement('div');
flipBoxFront.className = 'flip-box-front';
flipBoxFront.innerHTML = '<h2>Front Side</h2>';
flipBoxInner.appendChild(flipBoxFront);
var flipBoxBack = document.createElement('div');
flipBoxBack.className = 'flip-box-back';
flipBoxBack.innerHTML = '<h2>Back Side</h2>';
flipBoxInner.appendChild(flipBoxBack);
document.body.appendChild(flipBox);

var style = document.createElement('style');
style.innerHTML = 'body { font-family: Arial, Helvetica, sans-serif; } .flip-box { background-color: transparent; width: 300px; height: 200px; border: 1px solid #f1f1f1; perspective: 1000px; } .flip-box-inner { position: relative; width: 100%; height: 100%; text-align: center; transition: transform 0.8s; transform-style: preserve-3d; } .flip-box:hover .flip-box-inner { transform: rotateY(180deg); } .flip-box-front, .flip-box-back { position: absolute; width: 100%; height: 100%; -webkit-backface-visibility: hidden; backface-visibility: hidden; } .flip-box-front { background-color: #bbb; color: black; } .flip-box-back { background-color: dodgerblue; color: white; transform: rotateY(180deg); }';
document.head.appendChild(style);
/* clonar 2 veces */
var flipBoxClone = flipBox.cloneNode(true);
document.body.appendChild(flipBoxClone);
var flipBoxClone = flipBox.cloneNode(true);
document.body.appendChild(flipBoxClone);
var flipBoxClone = flipBox.cloneNode(true);
document.body.appendChild(flipBoxClone);
var flipBoxClone = flipBox.cloneNode(true);
document.body.appendChild(flipBoxClone);
/* float left */
var style = document.createElement('style');
style.innerHTML = '.flip-box { float: left; }';
document.head.appendChild(style);
/* renombrar cada front size numerado */
var flipBoxFront = document.getElementsByClassName('flip-box-front');
for (var i = 0; i < flipBoxFront.length; i++) {
  flipBoxFront[i].innerHTML = '<h2>Front Side ' + (i + 1) + '</h2>';
}
/* back side */
var flipBoxBack = document.getElementsByClassName('flip-box-back');
for (var i = 0; i < flipBoxBack.length; i++) {
  flipBoxBack[i].innerHTML = '<h2>Back Side ' + (i + 1) + '</h2>';
}
/* imagen random y cover en front side */
var flipBoxFront = document.getElementsByClassName('flip-box-front');
for (var i = 0; i < flipBoxFront.length; i++) {
  var img = document.createElement('img');
  img.src = 'https://source.unsplash.com/random/' + (i + 1) + '?w=800' + (i + 1);
  img.style.width = '100%';
  img.style.height = '100%';
  img.style.objectFit = 'cover';
  flipBoxFront[i].appendChild(img);
}
/* otra en back side */
var flipBoxBack = document.getElementsByClassName('flip-box-back');
for (var i = 0; i < flipBoxBack.length; i++) {
  var img = document.createElement('img');
